{
  "name": "synci",
  "version": "0.0.1",
  "contributors": [
    "Jona Abdinghoff (ZerataX)"
  ],
  "license": "Unlicense",
  "repository": "ZerataX/synci",
  "homepage": "https://syn.ci",
  "scripts": {
    "start": "polymer serve -p 8095",
    "live": "polymer serve -p 8095 & browser-sync start --no-open -proxy 'localhost:8095' --port 8096 --ui-port 8097 --files 'index.html, src/**/*.js, images/*'",
    "lint": "npm run lint:js",
    "lint:sass": "sass-lint 'src/**/*.scss' -v -q",
    "lint:js": "standard",
    "build": "polymer build --auto-base-path && gulp prpl-server",
    "build:static": "polymer build",
    "watch": "npm run watch:styling",
    "watch:styling": "nodemon -e scss -x \"npm run prepare:styling\"",
    "prepare": "npm run prepare:styling && npm run prepare:dependencies",
    "prepare:styling": "./scripts/build-styling.sh",
    "prepare:dependencies": "mkdir -p dist/ && find deps -maxdepth 1 -type f -name \"*.js\" -exec sh -c 'browserify \"deps/$(basename {})\" > \"dist/$(basename {})\"' \\;",
    "serve": "prpl-server --root server/build --port 8096",
    "serve:static": "polymer serve --port 5000 build/esm-bundled",
    "favicons": "./scripts/favicons.sh",
    "publish": "./scripts/publish-ipfs.sh",
    "test": "npm run lint && npm run test:unit && npm run test:integration",
    "test:integration": "mocha test/integration --timeout=10000",
    "test:unit": "polymer test --module-resolution=node --npm",
    "test:sauce": "polymer test --module-resolution=node --npm -s 'windows 10/microsoftedge@17' -s 'macos 10.13/safari@11'",
    "test:regenerate_screenshots": "mocha test/integration/screenshots-baseline/regenerate.js --timeout=15000"
  },
  "dependencies": {
    "@kuscamara/code-sample": "^3.0.2",
    "@polymer/app-layout": "^3.0.0",
    "@polymer/iron-icon": "^3.0.1",
    "@polymer/iron-icons": "^3.0.1",
    "@polymer/iron-image": "^3.0.2",
    "@polymer/paper-button": "^3.0.1",
    "@polymer/paper-card": "^3.0.1",
    "@polymer/paper-dialog": "^3.0.1",
    "@polymer/paper-icon-button": "^3.0.2",
    "@polymer/paper-input": "^3.0.2",
    "@polymer/paper-material": "^3.0.1",
    "@polymer/paper-ripple": "^3.0.1",
    "@polymer/paper-slider": "^3.0.1",
    "@polymer/paper-styles": "^3.0.1",
    "@polymer/polymer": "^3.1.0",
    "@webcomponents/webcomponentsjs": "^2.2.4",
    "lit-element": "^2.0.1",
    "pwa-helpers": "^0.9.0",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0"
  },
  "devDependencies": {
    "@polymer/test-fixture": "^4.0.2",
    "axe-core": "^3.0.0",
    "browser-sync": "^2.26.7",
    "browserify": "^16.5.0",
    "chai": "^4.1.2",
    "del": "^3.0.0",
    "gulp": "^4.0.0",
    "gulp-rename": "^1.3.0",
    "gulp-replace": "^1.0.0",
    "ipfs": "^0.37.1",
    "ipfs-pubsub-room": "^1.4.1",
    "mocha": "^5.2.0",
    "node-sass-import": "^2.0.1",
    "nodemon": "^1.19.2",
    "nodesass": "0.0.2-security",
    "pixelmatch": "^4.0.2",
    "polymer-cli": "^1.9.11",
    "polyserve": "^0.27.0",
    "prpl-server": "^1.4.0",
    "puppeteer": "^1.5.0",
    "sass": "^1.23.0-module.beta.1",
    "sass-lint": "^1.13.1",
    "standard": "^14.3.1",
    "tinyify": "^2.5.1",
    "wct-mocha": "^1.0.1"
  }
}
